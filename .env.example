# 도서관정보나루 API 인증키
# 발급: https://www.data4library.kr/
NEXT_PUBLIC_LIBRARY_API_KEY=your_library_api_key_here

# Kakao Map JavaScript API 키
# 발급: https://developers.kakao.com/
NEXT_PUBLIC_KAKAO_MAP_KEY=your_kakao_map_key_here

# 네이버 도서 검색 API 키
# 발급: https://developers.naver.com/apps/#/register
# ⚠️ Client Secret은 NEXT_PUBLIC_ 접두사를 붙이지 마세요! (서버 전용)
NAVER_CLIENT_ID=your_naver_client_id_here
NAVER_CLIENT_SECRET=your_naver_client_secret_here

# 기본 위치 설정 (서울시청)
NEXT_PUBLIC_DEFAULT_LAT=37.5665
NEXT_PUBLIC_DEFAULT_LNG=126.9780

# API Base URL
NEXT_PUBLIC_LIBRARY_API_BASE=http://data4library.kr/api
